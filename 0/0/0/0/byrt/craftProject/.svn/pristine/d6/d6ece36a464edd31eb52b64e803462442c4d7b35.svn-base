#focus {width:800px; height:800px; overflow:hidden; position:relative;}
#focus ul {height:270px; position:absolute;}
#focus ul li {float:left; width:800px; height:800px; overflow:hidden; position:relative;}
#focus ul li div {position:absolute; overflow:hidden;}
/*#focus ul li img {width:800px; height:800px;}*/
#focus .preNext {width:45px; height:90px; position:absolute; top:400px;cursor:pointer;background: url(../images/icon.png) no-repeat center;}
#focus .pre {background-position: -30px 0;position: absolute;left: 0px;top: 400px;}
#focus .next {background-position: -35px -60px;position: absolute;right: 0px;top: 400px;}